<entry key="script" type="xstring" value="# required libraries%%00010library(&quot;Rtsne&quot;)%%00010library(&quot;dplyr&quot;)%%00010library(&quot;ggplot2&quot;)%%00010%%00010# input%%00010data &lt;- knime.in%%00010design &lt;- knime.flow.in[[&quot;design&quot;]]%%00010design &lt;- trimws(strsplit(design,',')[[1]])%%00010perplexity &lt;- knime.flow.in[[&quot;perplexity&quot;]]%%00010%%00010# transpose because of tSNE design%%00010data_tSNE &lt;- as.data.frame(t(data))%%00010%%00010# info for conditions description%%00010df &lt;- data.frame(matrix(unlist(design), nrow=length(design), byrow=T))%%00010colnames(df) &lt;- c(&quot;design&quot;)%%00010info.norm = tibble(condition = factor(df$design))%%00010%%00010# tSNE itself%%00010data_tSNE[is.na(data_tSNE)] &lt;- 0%%00010#dim(data_tSNE)%%00010tsne_results &lt;- Rtsne(data_tSNE, perplexity=perplexity, check_duplicates = FALSE)%%00010%%00010# plotting...%%00010bla &lt;- as.data.frame(tsne_results$Y)%%00010colnames(bla) &lt;- c(&quot;tsne1&quot;,&quot;tsne2&quot;)%%00010info.norm$tsne1 &lt;- bla$tsne1%%00010info.norm$tsne2 &lt;- bla$tsne2%%00010plot_tSNE &lt;- ggplot(info.norm, aes(x = tsne1, y = tsne2, colour = condition)) + %%00010  geom_point(alpha = 1) + theme_bw()%%00010%%00010plot(plot_tSNE)%%00010"/>
